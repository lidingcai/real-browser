{
  "name": "min-browser in typescript",
  "version": "1.0.0",
  "description": "followed a version of https://github.com/minbrowser/min , but transfer to typescript",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "browser",
    "min"
  ],
  "license": "Apache-2.0",
  "author": "lidingcai <lidingcai@lidingcai.com>",
  "type": "module",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "dev": "vite",
    "electron:build": "npm run electron:prebuild && electron-builder",
    "electron:build:linux": "npm run electron:prebuild && electron-builder --linux",
    "electron:build:mac": "npm run electron:prebuild && electron-builder --mac",
    "electron:build:win": "npm run electron:prebuild && electron-builder --win",
    "electron:dev": "vite",
    "electron:prebuild": "vite build",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./ --max-warnings 0",
    "lint:fix": "eslint --ext .vue,.js,jsx,.ts,.tsx ./ --max-warnings 0 --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "10.7.2",
    "chokidar": "3.6.0",
    "dragula": "3.7.3",
    "expr-eval": "2.0.2",
    "i18next": "23.8.2",
    "json5": "2.2.3",
    "mousetrap": "1.6.5",
    "pdfjs-dist": "4.0.379",
    "quick-score": "0.2.0",
    "sortablejs": "1.15.2",
    "trusted-types": "2.0.0",
    "vue": "3.4.15",
    "vue-i18n": "9.9.1",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@types/dragula": "3.7.4",
    "@types/mousetrap": "1.6.15",
    "@types/node": "20.11.19",
    "@types/sortablejs": "1.15.7",
    "@types/trusted-types": "2.0.7",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "@vitejs/plugin-vue": "5.0.3",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "electron": "28.2.1",
    "electron-builder": "24.9.1",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-define-config": "2.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-vue": "9.21.1",
    "eslint-plugin-vue-scoped-css": "2.7.2",
    "tree-kill": "1.2.2",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-plugin-electron": "0.28.0",
    "vite-plugin-electron-renderer": "0.14.5",
    "vite-plugin-imagemin": "0.6.1",
    "vite-plugin-require-transform": "1.0.21",
    "vite-svg-loader": "5.1.0",
    "vue-eslint-parser": "9.4.2",
    "vue-tsc": "1.8.27"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  }
}
